{
  "package": {
    "productName": "notex-desktop",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "../build",
    "devPath": "http://0.0.0.0:3000",
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "io.github.FukudaTaiga.notex-desktop",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "https://github.com/FukudaTaiga",
      "category": "Education",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false,
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "label": "main",
        "title": "NoTeX",
        "url": "/",
        "width": 800,
        "height": 600,
        "resizable": true
      },
      {
        "label": "hidden",
        "title": "Hidden",
        "url": "/listen",
        "width": 600,
        "height": 450,
        "visible": false,
        "focus": false,
        "fileDropEnabled": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
